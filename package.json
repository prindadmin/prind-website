{
  "name": "prind-website",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.9",
    "bootstrap": "^5.0.1",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "s3Sync:dev": "aws s3 sync build/ s3://prind-website-dev --profile prind",
    "s3Sync:production": "aws s3 sync build/ s3://prind-website --profile prind",
    "cfInvalidate:dev": "aws cloudfront create-invalidation --distribution-id E1F42OFZQ4PIIS --paths /\\*  --profile prind",
    "cfInvalidate:production": "aws cloudfront create-invalidation --distribution-id E1AS0LJ3TQ51GU --paths /\\*  --profile prind",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:production": "env-cmd -f .env.production npm run build",
    "deploy:dev": "npm run build:dev && npm run s3Sync:dev  && npm run cfInvalidate:dev",
    "deploy:production": "npm run build:production && npm run s3Sync:production && npm run cfInvalidate:production",
    "deploy:all": "npm run deploy:production && npm run deploy:dev",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
